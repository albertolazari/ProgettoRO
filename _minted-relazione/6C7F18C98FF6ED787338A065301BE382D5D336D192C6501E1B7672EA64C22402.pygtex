\PYG{g+go}{s.t. soloCorsoAgonisti\PYGZob{}i in I, g in G\PYGZcb{}:}
\PYG{g+go}{    b[i,g,orarioCorsoAgonisti] + b[i,g,orarioCorsoAgonisti+1]}
\PYG{g+go}{    + l[i,g,orarioCorsoAgonisti] + l[i,g,orarioCorsoAgonisti+1]}
\PYG{g+go}{    \PYGZlt{}= (1 \PYGZhy{} a[i,g]) * M;}

\PYG{g+go}{s.t. vincoloMinAllenamentiAgonisti: sum\PYGZob{}g in G\PYGZcb{} t[g] \PYGZgt{}= minAllenamentiAgonisti;}
