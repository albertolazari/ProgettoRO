\PYG{g+go}{var d\PYGZob{}I\PYGZcb{} binary;}

\PYG{g+go}{maximize FO:}
\PYG{g+go}{    (sum\PYGZob{}g in G, o in O\PYGZcb{} boulder[g,o] * guadagnoCorsoBoulder)}
\PYG{g+go}{    + (sum\PYGZob{}g in G, o in O\PYGZcb{} lead[g,o] * guadagnoCorsoLead)}
\PYG{g+go}{    + (sum\PYGZob{}g in G\PYGZcb{} t[g] * guadagnoCorsoAgonisti)}
\PYG{g+go}{    + k * guadagnoCorsoOutdoor}
\PYG{g+go}{    \PYGZhy{} costoIstruttore * (}
\PYG{g+go}{        (sum\PYGZob{}i in I, g in G, o in O\PYGZcb{} (b[i,g,o] + l[i,g,o]))}
\PYG{g+go}{        + (sum\PYGZob{}i in I, g in G\PYGZcb{} 2 * a[i,g])}
\PYG{g+go}{        + (sum\PYGZob{}i in I\PYGZcb{} d[i] * durataCorsoOutdoor)}
\PYG{g+go}{    );}

\PYG{g+go}{s.t. istruttoreBoulder\PYGZob{}g in G, o in O\PYGZcb{}: sum\PYGZob{}i in I\PYGZcb{} b[i,g,o] * M \PYGZgt{}= boulder[g,o];}
\PYG{g+go}{s.t. istruttoreLead\PYGZob{}g in G, o in O\PYGZcb{}: sum\PYGZob{}i in I\PYGZcb{} l[i,g,o] * M \PYGZgt{}= lead[g,o];}
